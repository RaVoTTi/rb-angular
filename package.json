{
  "name": "robinbook",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:cus": "ng serve customer",
    "start:cus:prod": "http-server ./dist/customer -p 4000",
    "build:cus": "ng build customer",
    "build:stats:cus": "ng build customer --stats-json",
    "analyze:cus": "npm run build:stats:cus && webpack-bundle-analyzer dist/customer/stats.json",
    "watch": "ng build --watch --configuration development",
    "lhci:cus": "npm run build:cus && lhci autorun --config=lighthouserc.js",
    "test": "ng test",
    "images": "npm run images:resize && npm run images:compress",
    "images:resize": "node scripts/resize.mjs",
    "images:compress": "node scripts/compress.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/router": "^15.2.5",
    "@ngrx/component-store": "^15.4.0",
    "@ngrx/data": "^15.4.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/entity": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "bootstrap": "^5.3.0-alpha1",
    "bootstrap-icons": "^1.10.3",
    "jquery": "^3.6.4",
    "postcss": "^8.4.21",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.7.3",
    "tslib": "^2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.5",
    "@lhci/cli": "^0.11.0",
    "@ngrx/store-devtools": "^15.4.0",
    "@types/jasmine": "~4.3.1",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "typescript": "~4.8.2",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
